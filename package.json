{
  "name": "custom-package-name",
  "version": "0.0.7",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/codes-templates/npm-react#readme",
  "bugs": {
    "url": "https://github.com/codes-templates/npm-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codes-templates/npm-react.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "global.d.ts"
  ],
  "workspaces": [
    "playground",
    "docs"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build": "tsup && npm run build:css",
    "dev": "npm run dev -w playground",
    "dev:use-bundle": "run-p tsup-w dev-pack",
    "docs": "npm run start -w docs",
    "build:docs": "npm run build -w docs",
    "build:css": "npm run build:scss && npm run build:postcss && rimraf rf ./src/index.css",
    "build:scss": "sass --no-source-map --style=compressed src/styles/main.scss src/index.css",
    "build:postcss": "postcss src/index.css -o dist/index.css",
    "build:vite": "vite build && npm run build:css",
    "build:storybook": "build-storybook",
    "preview:docs": "npm run serve -w docs",
    "lint": "eslint --ext .ts,.tsx ./src",
    "tsup-w": "tsup --watch",
    "dev-pack": "npm run dev:usepack -w playground",
    "prepare": "chmod a+x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "stylelint": "stylelint src/**/*.css",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.3",
    "@storybook/react": "^6.5.12",
    "@swc/core": "^1.3.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitest/ui": "^0.24.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "bumpp": "^8.2.1",
    "core-js": "^3.25.5",
    "cssnano": "^5.1.13",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.17",
    "postcss-cli": "^10.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-nested": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-element-to-jsx-string": "^15.0.0",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-prettier": "^2.0.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-dts": "^1.6.5",
    "vitest": "^0.24.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  }
}
