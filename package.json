{
  "name": "custom-package-name",
  "version": "0.0.7",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/jerrywu001/react-npm-template#readme",
  "bugs": {
    "url": "https://github.com/jerrywu001/react-npm-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerrywu001/react-npm-template.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "global.d.ts"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "tsup && npm run build:css",
    "build:css": "npm run build:scss && npm run build:postcss",
    "build:css-watch": "concurrently \"npm run build:scss-watch\" \"npm run build:postcss-watch\"",
    "build:postcss": "postcss src/index.css -o dist/index.css",
    "build:postcss-watch": "postcss src/index.css -o dist/index.css -w",
    "build:scss": "sass --no-source-map --style=expanded src/styles/main.scss src/index.css",
    "build:scss-watch": "sass --no-source-map --style=expanded --watch src/styles/main.scss src/index.css",
    "build:storybook": "npm run build:storybook -w playground",
    "build:vite": "vite build && npm run build:css",
    "dev": "concurrently \"npm run build:css-watch\" \"npm run dev -w playground\"",
    "dev:pack": "concurrently \"npm run build\" \"npm run dev:usepack -w playground\"",
    "lint": "eslint --ext .ts,.tsx ./src",
    "prepare": "chmod a+x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "storybook": "start-storybook -p 6006",
    "stylelint": "stylelint src/**/*.css",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "@swc/core": "^1.2.242",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitest/ui": "^0.22.1",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "bumpp": "^8.2.1",
    "concurrently": "^7.3.0",
    "core-js": "^3.25.0",
    "cssnano": "^5.1.13",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "path": "^0.12.7",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-nested": "^5.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-element-to-jsx-string": "^15.0.0",
    "sass": "^1.54.5",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-prettier": "^2.0.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.22.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  }
}
