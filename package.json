{
  "name": "custom-package-name",
  "version": "0.0.7",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/codes-templates/npm-react#readme",
  "bugs": {
    "url": "https://github.com/codes-templates/npm-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codes-templates/npm-react.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "global.d.ts",
    "*.d.ts"
  ],
  "workspaces": [
    "playground",
    "docs"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build": "tsup && npm run build:css",
    "dev": "npm run dev -w playground",
    "dev:use-bundle": "run-p tsup-w dev-pack",
    "docs": "npm run start -w docs",
    "build:docs": "npm run build -w docs",
    "build:css": "npm run build:scss && npm run build:postcss && rimraf rf ./src/index.css",
    "build:scss": "sass --no-source-map --style=compressed src/styles/main.scss src/index.css",
    "build:postcss": "postcss src/index.css -o dist/index.css",
    "build:vite": "vite build && npm run build:css",
    "build:storybook": "cross-env NO_DTS=1 storybook build",
    "preview:docs": "npm run serve -w docs",
    "lint": "eslint --ext .ts,.tsx ./src",
    "tsup-w": "tsup --watch",
    "dev-pack": "npm run dev:usepack -w playground",
    "prepare": "chmod a+x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.6",
    "core-js": "^3.31.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/react-vite": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@swc/core": "^1.3.71",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitest/ui": "^0.33.0",
    "babel-loader": "^9.1.3",
    "browserslist-to-esbuild": "^1.2.0",
    "bumpp": "^9.1.1",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.1.0",
    "postcss-scss": "^4.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-element-to-jsx-string": "^15.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "storybook": "^7.1.1",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "tsup": "^7.1.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  }
}
